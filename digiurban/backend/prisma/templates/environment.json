          "label": "Localização GPS",
          "type": "map",
          "required": false
        },
        {
          "id": "photos",
          "label": "Fotos/Evidências",
          "type": "file-multiple",
          "accept": "image/*,video/*",
          "maxFiles": 10,
          "required": false
        },
        {
          "id": "priority",
          "label": "Urgência",
